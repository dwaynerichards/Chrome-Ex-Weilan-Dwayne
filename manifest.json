{
    "manifest_version": 2,
    "name": "thisNoise",
    "description": "where is the sound coming from",
    "version": "1.1",
    "background": {
        "service_worker": "background.js"
      },
    "browser_action": {
    "default_icon": "assets/LookSound.png",
    "default_popup": "assets/LookSound.png"
  },
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "notifications",
    "background",
    "contentSettings",
    "debugger",
    "desktopCapture",
    "storage"
  ]
}
